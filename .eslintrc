{
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {
      "__$styleInject": true,
      "ENV": true
    },
    "extends": "plugin:react/recommended",
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "no-console": [
          "error",
          {
            "allow": ["info", "warn", "error"]
          }
        ]
    }
}
